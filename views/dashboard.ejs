    <div class="container-fluid mt--7">
      <div class="row">
        <div class="col">
          <div class="card shadow">
            <div class="card-header bg-transparent">
              <div class="row align-items-center">
                <div class="col">
                  <h6 class="text-uppercase text-muted ls-1 mb-1">Envíos a festivales</h6>
                  <h2 class="mb-0">Envíos pendientes</h2>
                </div>
              </div>
            </div>
            <div class="card-body">
              <!-- Chart -->
              <table
                    id="table"
                    data-sort-name="date"
                    data-sort-order="desc"
                    data-search="true"
                    data-pagination="true"
                    data-locale="en-US">
                    <thead>
                      <tr>
                        <th data-field="date" data-sortable="true">Deadline</th>
                        <th data-field="festival" data-sortable="true">EntryFee</th>
                        <th data-field="envío" data-sortable="true">Sin EntryFee</th>
                        <th data-field="description" data-sortable="true">Número de envíos</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <th scope="row">1</th>
                        <td>Mark</td>
                        <td>Otto</td>
                        <td>@mdo</td>
                      </tr>
                      <tr>
                        <th scope="row">2</th>
                        <td>Mark</td>
                        <td>Twain</td>
                        <td>@mdooo</td>
                      </tr>
                    </tbody>
                  </table>
            </div>
          </div>
        </div>
