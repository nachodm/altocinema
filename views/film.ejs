<body>
  <!-- Sidenav -->
  <%- include ('partials/sidebar'); -%>
  <!-- Main content -->
  <div class="main-content">
    <!-- Page content -->
    <div class="container-fluid mt-4">
      <div class="row justify-content-center">
        <div class="col-xl-12">
          <div class="card bg-secondary shadow">
            <div class="card-header bg-white border-0">
              <div class="row align-items-center">
                <div class="col-8">
                  <h3 class="mb-0"><%=title%></h3>
                </div>
              </div>
            </div>
            <div class="card-body">
              <form id="addFilmForm" action="/updateFilm" method="POST">
                <input type="hidden" id="id" name="id" value="<%=film.id%>">
                <div class="pl-lg-4">
                  <div class="row">
                    <div class="col-lg-6 my--2">
                      <div class="form-group">
                        <label class="form-control-label" for="title">Título original</label>
                        <input type="text" id="title" class="form-control form-control-alternative form-control-sm" name="title" value="<%=film.title%>" required>
                        </div>
                    </div>
                    <div class="col-lg-6 my--2">
                      <div class="form-group">
                        <label class="form-control-label" for="engtitle">Título en inglés</label>
                        <input type="text" id="engtitle" class="form-control form-control-alternative form-control-sm" name="engtitle" value="<%=film.engtitle%>" required>
                      </div>
                    </div>
                    <div class="col-lg-2 my--2">
                      <div class="form-group">
                        <label class="form-control-label" for="year">Año</label>
                        <input type="text" id="year" class="form-control form-control-alternative form-control-sm" name="year" value="<%=film.year%>"required>
                        <% if (locals.messages && messages.errors && messages.errors.name) { %>
                          <strong class="invalid-feedback" style="display: block"><%= messages.errors.name %></strong>
                        <% } %>
                      </div>
                    </div>
                    <div class="col-lg-2 my--2">
                      <div class="form-group">
                        <label class="form-control-label" for="productiondate">Fecha</label>
                        <input type="date" id="date" class="form-control form-control-alternative form-control-sm" name="date" value="<%=film.date%>" required>
                        <% if (locals.messages && messages.errors && messages.errors.name) { %>
                          <strong class="invalid-feedback" style="display: block"><%= messages.errors.name %></strong>
                        <% } %>
                      </div>
                    </div>
                    <div class="col-lg-2 my--2">
                      <div class="form-group">
                        <label class="form-control-label" for="color">Color</label>
                        <select  class="form-control form-control-alternative form-control-sm" id="color" name="color" required>
                          <option value="color" <%= film.color === 'color' ? 'selected': '' %>>Color</option>
                          <option value="ByN" <%= film.color === 'ByN' ? 'selected': '' %>>Blanco y Negro</option>
                        </select> 
                      </div>
                    </div>
                    <div class="col-lg-3 my--2">
                      <div class="form-group">
                        <label class="form-control-label" for="animationtechnique">Técnica de animación</label>
                        <input type="text" id="animationtechnique" class="form-control form-control-alternative form-control-sm" name="animationtechnique" value="<%=film.animationtechnique%>">
                      </div>
                    </div>
                    <div class="col-lg-3 my--2">
