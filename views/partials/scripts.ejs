<script>
  $(document).ready(updateFinalFee);
  $("#waiver").change(updateFinalFee); 
  $("#disc").change(updateFinalFee); 
  $("#fee").change(updateFinalFee); 

  function updateFinalFee(){
    
      let waiver = $("#waiver").val()
      if (waiver == "y1" || waiver == "y2") {
        $("#disc").prop('disabled', false); 
        let desc = $("#disc").val()/100;
        let final = desc * $("#fee").val();
        $("#final").val(final);
      }
      else {
        $("#disc").prop('disabled', true); 
        $("#final").val($("#fee").val());
      }
  }
</script>

